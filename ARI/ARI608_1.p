tff(c_type,type,
    c: $int ).

tff(f_mon_implies_trans,conjecture,
    ( ( ! [X: $int,Y: $int] :
          ( $lesseq(X,Y)
         => $lesseq(f(X),f(Y)) )
      & $lesseq(a,b)
      & $less(b,c) )
   => $lesseq(f(a),f(c)) ) ).

